#include<conio.h>
#include<stdio.h>
int main()
{
	int i,j;
	clrscr();
	printf("\n the star patterns\n");
	for(i=0;i<5;i++)
	{
		for(j=i;j<5;j++)
			printf("\t");
		printf("$");
		for(j=0;j<i;j++)
		{       printf("\t");
			printf("\t");
			printf("$");
		}
		printf("\n");
	}
	 printf("\n\n");
	 for(i=0;i<5;i++)
	{
		for(j=0;j<5;j++)
		printf("\t");
		printf("$");
		for(j=0;j<i;j++)
		{printf("\t");
		printf("$");
		}
		printf("\n");
	}



	getch();
	return 0;
	}
